{
    "languageserver": {
        "clangd": {
            "command": "clangd",
            "args": ["--background-index"],
            "filetypes": ["c", "cpp", "objc", "objcpp"]
        },
        "golang": {
            "command": "gopls",
            "rootPatterns": ["go.mod"],
            "disableWorkspaceFolders": true,
            "filetypes": ["go"]
        }
    }
}
